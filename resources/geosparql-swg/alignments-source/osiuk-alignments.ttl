PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX spatialuk: <http://data.ordnancesurvey.co.uk/ontology/spatialrelations/>
PREFIX spatialukgeom: <http://data.ordnancesurvey.co.uk/ontology/geometry/>

spatialukgeom:AbstractGeometry
    owl:equivalentClass geo:Geometry ;
.

spatialukgeom:asGML
    owl:equivalentProperty geo:asGML ;
.

spatialukgeom:extent
    owl:equivalentProperty geo:hasBoundingBox ;
.

spatialuk:contains
    owl:equivalentProperty geo:sfContains ;
.

spatialuk:disjoint
    owl:equivalentProperty geo:sfDisjoint ;
.

spatialuk:equals
    owl:equivalentProperty geo:sfEquals ;
.

spatialuk:touches
    owl:equivalentProperty geo:sfTouches ;
.

spatialuk:within
    owl:equivalentProperty geo:sfWithin ;
.
