PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX juso: <http://rdfs.co/juso/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX sf: <http://www.opengis.net/ont/sf#>

juso:Feature
    owl:equivalentClass geo:Feature ;
.

juso:Geometry
    owl:equivalentClass geo:Geometry ;
.

juso:Point
    owl:equivalentClass sf:Point ;
.

juso:SpatialThing
    owl:equivalentClass geo:SpatialObject ;
.

juso:geometry
    owl:equivalentProperty geo:hasGeometry ;
.

juso:parent
    rdfs:subPropertyOf geo:sfWithin ;
.

juso:political_division
    rdfs:subPropertyOf geo:sfContains ;
.

juso:within
    owl:equivalentProperty geo:sfWithin ;
.
