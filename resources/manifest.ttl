# This file contains a machine-readable listing of the contents of this repository's vocabulary resources
# according to the Prez Manifest (https://prez.dev) model.

PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX mrr: <https://prez.dev/ManifestResourceRoles/>
PREFIX prez: <https://prez.dev/>
PREFIX prof: <http://www.w3.org/ns/dx/prof/>
PREFIX schema: <https://schema.org/>

[]
    a prez:Manifest ;
    prof:hasResource
        [
            prof:hasArtifact "catalogue.ttl" ;
            prof:hasRole mrr:CatalogueData ;
            schema:description "The definition of, and medata for, the container which here is a schema:Catalog object" ;
            schema:name "Catalogue Definition"
        ] ,
        [
            # dcterms:conformsTo <https://linked.data.gov.au/def/vocpub> ;  # standardised VocPub validation
            dcterms:conformsTo "vocpub-ogc.ttl" ;  # specialised OGC validation
            prof:hasArtifact "vocabs/**/*.ttl" ;
            prof:hasRole mrr:ResourceData ;
            schema:description "Vocabulary objects in RDF (Turtle) files in the vocabs/ folder and subfolders. All vocabs conform to the OGC's variant of the VocPub profile of SKOS" ;
            schema:name "Resource Data"
        ] ,
        [
            prof:hasArtifact "labels.ttl" ;
            prof:hasRole mrr:IncompleteCatalogueAndResourceLabels ;
            schema:description "An RDF files containing all the labels for the container content" ;
            schema:name "Labels" ;
        ] ,
        [
            prof:hasArtifact "agents.ttl" ;
            prof:hasRole mrr:IncompleteCatalogueAndResourceLabels ;
            schema:description "An RDF files containing labelling information for agents in the vocab and catalogue resources" ;
            schema:name "Agent information" ;
        ] ;
.